set width 96
set height 24

set base [file rootname [file tail $argv0]]
set send_human {.1 .3 1 .05 0.5}
set stty_init "cols $width rows $height"

spawn asciinema rec --overwrite ${base}.cast
expect "\$ "
send -h "k6x run script-with.js\r"
expect "\$ "
sleep 2
send -h "k6x run script-with.js\r"
expect "\$ "
sleep 2
send \x04
expect eof
